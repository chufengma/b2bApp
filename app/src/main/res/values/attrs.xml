<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TaoToolBar">
        <attr name="tool_bar_title"  format="string|reference" />
        <attr name="tool_bar_bg"  format="color|reference" />
        <attr name="tool_bar_left"  format="reference" />
        <attr name="tool_bar_right"  format="reference" />
        <attr name="tool_bar_icon_size"  format="dimension" />
    </declare-styleable>

    <declare-styleable name="XListView">
        <attr name="headerOffset" format="dimension" />
        <attr name="footerOffset" format="integer" />
        <attr name="hintTextColor" format="color" />
        <attr name="displayUpdateTime" format="boolean" />
        <attr name="showFooterArrow" format="boolean" />
        <attr name="headerHintNormal" format="string|reference" />
        <attr name="headerHintReady" format="string|reference" />
        <attr name="headerHintLoading" format="string|reference" />
        <attr name="footerHintNormal" format="string|reference" />
        <attr name="footerHintReady" format="string|reference" />
        <attr name="footerHintLoading" format="string|reference" />
    </declare-styleable>
</resources>