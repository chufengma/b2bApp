<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape>
            <corners android:radius="5dip" />
            <stroke android:color="@color/login_bg_disable" android:width="2dip"/>
            <solid android:color="@color/login_bg_disable" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="5dip" />
            <stroke android:color="@color/login_btn_border" android:width="2dip"/>
            <solid android:color="@color/login_btn_border" />
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="5dip" />
            <stroke android:color="@color/login_btn_border" android:width="2dip"/>
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>
</selector>